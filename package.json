{
  "name": "vimond-replay",
  "version": "0.6.9",
  "author": "Tor Erik Alr√¶k",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://vimond.jfrog.io/vimond/api/npm/npm-local/"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "guide.md",
    "README.md"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-lib": "npm run build-es5 && npm run build-flow && npm run build-css",
    "build-es5": "babel src/replay -d lib --copy-files --source-maps",
    "build-flow": "flow-copy-source src/replay lib",
    "build-css": "postcss src/replay/replay-default.css -o lib/index.css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react-feather": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-flow": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.46",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "npm:enzyme-react-adapter-future",
    "flow-bin": "0.74.0",
    "flow-copy-source": "^1.3.0",
    "postcss-cli": "^5.0.0",
    "postcss-import": "^11.1.0",
    "postcss-preset-env": "^5.1.0",
    "postcss-url": "^7.3.2",
    "prettier": "^1.12.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.1",
    "test-cli": "^0.1.0",
    "vimond-videostreamer-premium": "^8.19.300"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "parser": "flow",
    "jsxBracketSameLine": true
  },
  "resolutions": {
    "enzyme-adapter-react-16/react-test-renderer": "^16.3.2"
  }
}
